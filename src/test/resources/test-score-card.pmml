<PMML xmlns="http://www.dmg.org/PMML-4_3" version="4.3">
    <Header/>
    <DataDictionary>
        <DataField name="age" optype="continuous" dataType="integer"/>
        <DataField name="realName" optype="categorical" dataType="string" />
        <DataField name="score" optype="continuous" dataType="double" />
    </DataDictionary>
    <Scorecard modelName="UserInfoScore" functionName="regression" initialScore="1000">
        <MiningSchema>
            <MiningField name="age" invalidValueTreatment="asMissing"/>
            <MiningField name="realName" invalidValueTreatment="asMissing"/>
            <MiningField name="score" usageType="predicted"/>
        </MiningSchema>
        <Output>
            <OutputField name="reason1" rank="1" feature="reasonCode" dataType="string" optype="categorical"/>
            <OutputField name="reason2" rank="2" feature="reasonCode" dataType="string" optype="categorical"/>
        </Output>
        <Characteristics>
            <Characteristic name="ageScore" reasonCode="RC1" baselineScore="0">
                <Attribute partialScore="10">
                    <SimplePredicate field="age" operator="isMissing" />
                </Attribute>
                <Attribute partialScore="20">
                    <SimplePredicate field="age" operator="lessThan" value="18" />
                </Attribute>
                <Attribute partialScore="30">
                    <CompoundPredicate booleanOperator="and">
                        <SimplePredicate field="age" operator="greaterOrEqual" value="18"/>
                        <SimplePredicate field="age" operator="lessThan" value="30"/>
                    </CompoundPredicate>
                </Attribute>
                <Attribute partialScore="40">
                    <CompoundPredicate booleanOperator="and">
                        <SimplePredicate field="age" operator="greaterOrEqual" value="30" />
                        <SimplePredicate field="age" operator="lessThan" value="45" />
                    </CompoundPredicate>
                </Attribute>
                <Attribute partialScore="50">
                    <SimplePredicate field="age" operator="greaterOrEqual" value="45" />
                </Attribute>
            </Characteristic>
            <Characteristic name="realNameScore" reasonCode="RC2" baselineScore="0">
                <Attribute partialScore="100">
                    <SimplePredicate field="realName" operator="isMissing" />
                </Attribute>
                <Attribute partialScore="200">
                    <SimplePredicate field="realName" operator="equal" value="Wang" />
                </Attribute>
                <Attribute partialScore="300">
                    <SimplePredicate field="realName" operator="equal" value="Tom" />
                </Attribute>
                <Attribute partialScore="400">
                    <SimplePredicate field="realName" operator="equal" value="Cate" />
                </Attribute>
            </Characteristic>
        </Characteristics>
    </Scorecard>
</PMML>